{
  "name": "@keywcorp.com/little-differ",
  "version": "1.0.5",
  "description": "A library that diffs two JSON documents and output one JSON document describing both documents and their diffs",
  "main": "lib/littleDiffer.js",
  "scripts": {
    "babel": "babel littleDiffer.js --out-dir lib",
    "build": "npm run babel; node littleDiffer",
    "test": "npm run babel ;ava test.js",
    "lint": "eslint --config .eslintrc.json test.js 'littleDiffer.js'"
  },
  "author": "Juliet Adams",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "ava": "1.0.0-beta.4",
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "pre-commit": "^1.2.2"
  }
}
