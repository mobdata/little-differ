    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.12.0",
    "pre-commit": "^1.2.2"
  }
